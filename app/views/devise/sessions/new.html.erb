<h2>Log in</h2>

<form role="form" action=<%= user_session_path%> method="post">
  <div class="form-group">
    <label for="InputEmail1">Email address</label>
    <input type="email" class="form-control" id="InputEmail1" name ="user[email]" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="InputPassword1">Password</label>
    <input type="password" class="form-control" id="InputPassword1" name= "user[password]" placeholder="Password">
  </div>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token -%>
  <button type="submit" class="btn btn-default">Submit</button>
</form>



