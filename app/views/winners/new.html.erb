<form role="form" action=<%= winners_path%> method="post">
  <div class="form-group">
    <label for="WinForm">Winning Voucher Code</label>
    <input type="text" class="form-control" id="WinForm" name ="voucher" placeholder="Enter voucher code">
  </div>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token -%>
  <button type="submit" class="btn btn-default">Submit</button>
</form>